{
  "name": "@bootstrap-vue/v-b-toggle",
  "version": "1.0.0",
  "meta": {
    "title": "Toggle",
    "description": "Облегченная директива для переключения состояния видимости для сворачивания и боковых панелей по идентификатору. Он автоматически обрабатывает атрибуты доступности элемента триггера.",
    "directive": "VBToggle",
    "expression": [
      "String",
      "Array"
    ],
    "arg": {
      "version": "2.14.0",
      "pattern": "[a-zA-Z][a-zA-Z0-9_\\-]*",
      "description": "Идентификатор компонента для переключения",
      "required": false
    },
    "modifiers": [
      {
        "name": "{componentId}",
        "pattern": "[a-zA-Z][a-zA-Z0-9_\\-]*",
        "description": "Идентификатор компонента для переключения"
      }
    ]
  }
}
